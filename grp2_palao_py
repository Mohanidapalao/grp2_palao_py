# grp2_palao_ex2.py

# My first simple script
print("Hello, I'm Mohanida Magidala Palao! BSCS 3F")

# Python Variables
number = 2
name = "Mohanida"
print("Number:", number)
print("Name:", name)

# Variable Names
cecille = "Cecille"
catherine = "Catherine"
jonalyn = "Jonalyn"
meriel = "Meriel"
print("Group Members:", cecille, catherine, jonalyn, meriel)

# Many Values to Multiple Variables
a, b, c = 1, 2, 3
print("a:", a, "b:", b, "c:", c)

# One Value to Multiple Variables
x = y = z = 10
print("x:", x, "y:", y, "z:", z)
